from sklearn import datasets
from sklearn.model_selection import train_test_split
from sklearn import svm
from sklearn import metrics

datos = datasets.load_breast_cancer(as_frame=True)

# print(datos.frame.isna().sum())

# print(datos.frame.describe())

# datos.data.shape

X = datos.data
Y = datos.target

X_ent,X_pru, Y_ent, Y_pru = train_test_split(X,Y, test_size=.2)

model = svm.SVC()
model.fit(X_ent, Y_ent)

predi = model.predict(X_pru)

print("Exactitud: ", metrics.accuracy_score(Y_pru, predi))
print("Precision: ", metrics.precision_score(Y_pru, predi))